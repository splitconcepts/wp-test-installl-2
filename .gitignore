
# ignore all files starting with .
!.gitignore
!.gitattributes
# ignore all files that start with ~
~*
# ignore old or backed up files
*.old
*.bak
# ignore OS generated files
ehthumbs.db
Thumbs.db
.DS_Store
# ignore Editor files
*.sublime-project
*.sublime-workspace
*.komodoproject
local-adminer-*
local-xdebuginfo.php
# ignore log files and databases
error_log
*.log
*.sql
*.sqlite
# ignore compiled files
*.com
*.class
*.dll
*.exe
*.o
*.so
# ignore packaged files
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip
# ignore everything in the uploads directory:
wp-content/uploads/
# ignore additional wp-content items
wp-content/upgrade/
wp-content/updraft/
wp-content/smush-webp
wp-content/w3tc-config
wp-content/cache
wp-content/mu-plugins/split-*
wp-content/mu-plugins/wp-migrate-*
wp-content/mu-plugins/mode-wp-page-cache-loader.php
wp-content/plugins/admin_ips.txt
# ignore drop ins
wp-content/db.php
wp-content/advanced-cache.php
wp-content/object-cache.php
# ignore these plugins from the core
wp-content/plugins/stop-emails
wp-content/plugins/disable-emails
# ignore specific themes
wp-content/themes/twenty*
# ignore vendor/dependency directories/files
node_modules/
/vendor/
*.code-workspace
web.config
# ignore wp-config
wp-config.php
/wp-content/
/wp-admin/
/wp-includes/